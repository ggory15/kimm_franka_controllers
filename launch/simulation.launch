<launch>
  <arg name="pub_mode" default="false"/>
  <arg name="controller" default="true"/>
  <param name="robot_group" type="string" value="ns1"/>
  <param name="issimulation" type="bool" value="true"/>
  
  <node name="mujoco_ros" pkg="mujoco_ros" type="mujoco_ros" required="true" respawn="false" output="screen">
    <param name="license" type="string" value="$(env HOME)/.mujoco/mjkey.txt" />
    <param name="pub_mode" value="$(arg pub_mode)"/>
    <param name="model_file" type="string" value="$(find franka_panda_description)/franka_single/panda_arm_hand_single.xml"/>
    
  </node>
    <node name="kimm_franka_controllers_simulation" pkg="kimm_franka_controllers" args="-keyboard" type="kimm_franka_controllers_simulation" output="screen">
  </node>

  <group ns ="ns1">
    <param name="robot_urdf_path" type="string" value="$(find franka_panda_description)"/>
    <param name="robot_urdf" type="string" value="/franka_single/panda_arm_hand_l.urdf"/>
  </group>
</launch>
